<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<handlers>
			<clear />
			<add name="StaticFiles" path="*" verb="*" modules="StaticFileModule" resourceType="Either" requireAccess="None" />
		</handlers>
		<staticContent>
			<clientCache cacheControlMaxAge ="31.00:00:00" cacheControlMode="UseMaxAge" />
		</staticContent>
	</system.webServer>
</configuration>